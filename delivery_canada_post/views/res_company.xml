<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <record id="view_company_form_inherited" model="ir.ui.view">
            <field name="name">res.company.form inherited</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form" />
            <field name="type">form</field>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page string="Shipping Information" >
                        <group>
                            <group>
                                <field name="canadapost_certificate_number" />
                                <field name="canadapost_licence_number" />
                            </group>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
